proc DOINVUNCALL(IN)           (* forward: inverse uncall snoc *)
  uncall SNOC(OUT) <= IN;
  return OUT;

proc SNOC((X.Y))               (* append Y as last element to list X *)
  if =? X nil
  then X <= (Y.nil);           (* make one-element list *)
  else (H.X) <= X;    
       X <= call SNOC((X.Y));  (* recursive descent over X *)
       X <= (H.X);
  fi =? (tl X) nil;            (* one-element list? *)
  return X